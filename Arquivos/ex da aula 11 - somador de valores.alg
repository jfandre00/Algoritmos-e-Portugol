algoritmo "semnome"
// Função :
// Autor :
// Data : 8/11/2023
// Seção de Declarações 
var

N, S, C : Inteiro

inicio
// Seção de Comandos 

      S <- 0
   Para C <- 1 ate 5 faca
      EscrevaL("Digite um valor: ")
      Leia(N)
      S <- S + N
   FimPara
   EscrevaL("A soma dos valores é ", S)


fimalgoritmo

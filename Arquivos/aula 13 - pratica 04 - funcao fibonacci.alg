algoritmo "FibonacciProcedimento"
// Função :
// Autor :
// Data : 8/21/2023
// Seção de Declarações 
var
  I, T1, T2, T3: Inteiro
Funcao ProximoFibonacci(var N1, N2: inteiro) : inteiro
Var
   N3: Inteiro
Inicio
   N3 <- N1 + N2
   N1 <- N2
   N2 <- N3
   Retorne N3
FimFuncao
  
  
inicio
// Seção de Comandos 
 T1 <- 0
 EscrevaL(T1)
 T2 <- 1
 EscrevaL(T2)
 Para I <- 3 ate 10 faca
    T3 <- ProximoFibonacci(T1, T2)
    EscrevaL(T3)
 FimPara



fimalgoritmo

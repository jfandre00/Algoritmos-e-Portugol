algoritmo "semnome"
// Função :
// Autor :
// Data : 8/3/2023
// Seção de Declarações 
var

numero, cont : inteiro

inicio

numero <- 1
cont <- 30

Enquanto (cont >= numero) faca

Se (cont%4 = 0) entao
Escreva("[",cont," ]")
Senao

Escreva(cont, " ")
FimSe

cont <- cont - 1
FimEnquanto
// Seção de Comandos 
fimalgoritmo
